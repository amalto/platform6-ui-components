{
  "name": "@platform6/components-react",
  "version": "0.0.1",
  "description": "React specific wrapper for bulmil",
  "repository": {
    "type": "git",
    "url": "https://github.com/amalto/platform6-ui-components"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "rollup": "rollup -c",
    "tsc": "tsc -p ."
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "dependencies": {
    "@platform6/components": "0.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@types/jest": "26.0.13",
    "@types/node": "14.6.4",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "jest": "^26.4.2",
    "jest-dom": "^4.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "4.0.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
