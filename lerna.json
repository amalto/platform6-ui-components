{
    "lerna": "7.1.4",
    "packages": [
        "components/*"
    ],
    "command": {
        "init": {
            "exact": true
        },
        "version": {
            "npmClientArgs": [
                "--legacy-peer-deps"
            ],
            "conventionalCommits": true
        },
        "publish": {
            "directory": "{workspaceRoot}/{projectRoot}/dist"
        }
    },
    "private": false,
    "version": "independent"
}