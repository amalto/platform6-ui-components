    "@babel/register": "^7.0.0",
    "@types/base-64": "0.1.2",
    "@types/classnames": "^2.2.3",
    "@types/jquery": "^3.3.31",
    "@types/react": "15.6.10",
    "@types/react-dom": "15.5.6",
    "ava": "1.2.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-15": "1.3.0",
    "nyc": "^15.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "15.6.2",
    "sinon": "4.5.0",
    "ts-loader": "8.4.0",
    "typescript": "4.7.4",
    "webpack": "4.41.2"
