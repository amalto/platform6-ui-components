{
    "name": "@amalto/text-input",
    "displayName": "TextInput",
    "description": "Text input used on platform6.",
    "version": "1.4.29",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/amalto/platform6-ui-components"
    },
    "keywords": [
        "user-interface",
        "react",
        "typescript"
    ],
    "main": "./build/index.js",
    "typings": "./build/index.d.ts",
    "author": "Amalto Technologies",
    "scripts": {
        "setup": "npm install",
        "build": "rm -rf ./build/; tsc",
        "test": "ava ./test/test.js --verbose"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@amalto/help": "1.4.23",
        "classnames": "2.2.5",
        "react": "15.6.2",
        "react-dom": "15.6.2",
        "react-redux": "5.0.6",
        "redux": "3.7.2",
        "redux-form": "6.6.1",
        "uuid": "3.2.1"
    },
    "devDependencies": {
        "@ava/babel-preset-stage-4": "1.1.0",
        "@types/classnames": "^2.2.3",
        "@types/react": "15.6.10",
        "@types/react-dom": "15.5.6",
        "@types/redux-form": "6.6.1",
        "ava": "0.24.0",
        "babel-preset-react": "6.24.1",
        "babel-register": "6.26.0",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-15": "1.0.5",
        "react-addons-test-utils": "15.6.2",
        "react-test-renderer": "15.6.2",
        "sinon": "4.2.2",
        "typescript": "2.6.2",
        "webpack": "3.10.0"
    },
    "ava": {
        "concurrency": 5,
        "failFast": true,
        "failWithoutAssertions": false,
        "powerAssert": true,
        "require": [
            "babel-register"
        ],
        "babel": {
            "presets": [
                "@ava/stage-4",
                "react"
            ]
        }
    }
}
