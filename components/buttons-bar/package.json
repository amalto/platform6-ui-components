{
  "name": "@amalto/buttons-bar",
  "displayName": "ButtonsBar",
  "description": "Buttons bar used in platform6.",
  "version": "1.9.92",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amalto/platform6-ui-components"
  },
  "keywords": [
    "user-interface",
    "react",
    "typescript"
  ],
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "author": "Amalto Technologies",
  "scripts": {
    "setup": "npm install",
    "build": "rm -rf ./build/; tsc",
    "test": "nyc ava ./test/test.js --verbose"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "2.0.0",
    "@ava/babel-preset-transform-test-files": "^4.0.1",
    "@babel/core": "^7.24.6",
    "@babel/plugin-syntax-jsx": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/register": "^7.0.0",
    "@types/jquery": "^3.3.31",
    "@types/jsdom": "^21.1.7",
    "@types/react": "15.6.10",
    "@types/react-dom": "15.5.6",
    "ava": "^6.1.3",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-15": "1.4.4",
    "jsdom": "^24.1.0",
    "nyc": "^15.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "15.6.2",
    "sinon": "4.5.0",
    "ts-loader": "8.4.0",
    "typescript": "5.4.5",
    "webpack": "4.41.2"
  },
  "dependencies": {
    "@amalto/helpers": "1.9.90",
    "@amalto/wordings": "1.9.34",
    "classnames": "^2.5.1",
    "react": "15.6.2"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "@babel/register",
      "./test/utils.js"
    ],
    "babel": {
      "extensions": [
        "js",
        "jsx"
      ],
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "@ava/babel-preset-stage-4",
          "@babel/preset-react"
        ]
      }
    }
  },
  "gitHead": "cfaae955fad45a25eaa926595a2c7af85c212df4"
}
