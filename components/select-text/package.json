{
  "name": "@amalto/select-text",
  "displayName": "SelectText",
  "description": "Select and text input used on platform6.",
  "version": "1.9.93",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amalto/platform6-ui-components"
  },
  "keywords": [
    "user-interface",
    "react",
    "typescript"
  ],
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "author": "Amalto Technologies",
  "scripts": {
    "setup": "npm install",
    "build": "rm -rf ./build/; tsc",
    "test": "nyc ava ./test/test.js --verbose"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@amalto/help": "1.9.27",
    "@amalto/helpers": "1.9.88",
    "@amalto/typings": "1.9.69",
    "classnames": "^2.2.6",
    "radium": "^0.25.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "2.0.0",
    "@ava/babel-preset-transform-test-files": "^4.0.1",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/classnames": "^2.2.3",
    "@types/react": "15.6.10",
    "@types/react-dom": "15.5.6",
    "ava": "1.2.1",
    "babel-preset-react": "6.24.1",
    "bootstrap": "^4.3.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-15": "1.3.0",
    "modernizr": "^3.8.0",
    "nyc": "^15.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "15.6.2",
    "sinon": "4.5.0",
    "ts-loader": "8.4.0",
    "typescript": "4.7.4",
    "webpack": "4.41.2"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "@babel/register",
      "./test/utils.js"
    ],
    "babel": {
      "extensions": [
        "js",
        "jsx"
      ],
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "@ava/babel-preset-stage-4",
          "@babel/preset-react"
        ]
      }
    }
  },
  "gitHead": "cfaae955fad45a25eaa926595a2c7af85c212df4"
}
