{
  "name": "@amalto/file-upload-input",
  "displayName": "FileUploadInput",
  "description": "Input allowing you to upload a file.",
  "version": "1.10.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amalto/platform6-ui-components"
  },
  "keywords": [
    "user-interface",
    "react",
    "typescript"
  ],
  "main": "./build/index.js",
