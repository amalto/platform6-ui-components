  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@amalto/help": "1.9.27",
    "@amalto/helpers": "1.9.86",
    "@amalto/typings": "1.9.69",
    "@amalto/wordings": "1.9.34",
    "classnames": "^2.2.6",
    "jquery": "^3.4.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-form": "6.6.1"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "2.0.0",
    "@ava/babel-preset-transform-test-files": "^4.0.1",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/bootstrap": "3.3.36",
    "@types/classnames": "^2.2.3",
    "@types/jquery": "^3.3.31",
    "@types/react": "15.6.10",
    "@types/react-dom": "15.5.6",
    "ava": "1.2.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-15": "1.3.0",
    "moment": "2.29.4",
    "nyc": "^15.1.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "15.6.2",
    "sinon": "4.5.0",
    "ts-loader": "8.4.0",
    "typescript": "4.7.4",
    "webpack": "4.41.2"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "@babel/register",
      "./test/utils.js"
    ],
    "babel": {
      "extensions": [
        "js",
        "jsx"
      ],
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "@ava/babel-preset-stage-4",
          "@babel/preset-react"
        ]
      }
    }
  },
  "gitHead": "33f7a6fae6e176a0592ab62c68682aef3e73a752",
  "lerna": {
    "command": {
      "publish": {
        "assets": [
          "package.json",
          "README.md"
        ]
      }
    }
  }
}
