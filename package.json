{
    "name": "@amalto/platform6-ui-components",
    "version": "1.7.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/amalto/platform6-ui-components"
    },
    "keywords": [
        "user-interface",
        "react",
        "typescript"
    ],
    "description": "Platform 6 UI components provided to develop the user interface of services.",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "lerna clean --yes; rm -rf node_modules package-lock.json; npm cache clean --force",
        "build": "lerna run build --no-sort",
        "clean-install": "npm run clean && npm install --legacy-peer-deps",
        "format": "npx prettier --write components/**/* typescript/**/*",
        "check": "npx prettier --check components/**/* typescript/**/*",
        "test": "nyc ava --verbose",
        "update_p6_version": "node update_platform6_version.js",
        "release": "lerna publish --yes --exact --registry=https://registry.npmjs.org/ --no-git-reset patch -- --production",
        "release:version": "lerna publish --yes --registry=https://registry.npmjs.org/ --no-git-reset patch --exact -- --production",
        "release:force": "lerna publish --yes --registry=https://registry.npmjs.org/ --no-git-reset patch --exact --force-publish=* -- --production",
        "setup": "npm install",
        "styleguide": "npx styleguidist server --verbose",
        "styleguide:build": "styleguidist build -- --no-deprecation"
    },
    "workspaces": [
        "components/service-helpers",
        "components/helpers",
        "components/wordings",
        "components/**"
    ],
    "optionalDependencies": {
        "fsevents": "~2.3.2"
    },
    "devDependencies": {
