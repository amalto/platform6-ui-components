        ],
        "concurrency": 5,
        "failFast": true,
        "failWithoutAssertions": false,
        "powerAssert": true,
        "require": [
            "@babel/register",
            "./test/utils.js"
        ],
        "babel": {
            "extensions": [
                "js",
                "jsx"
            ],
            "testOptions": {
                "plugins": [
                    "@babel/plugin-syntax-jsx"
