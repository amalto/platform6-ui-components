{
    "name": "@amalto/platform6-ui-components",
    "version": "1.7.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/amalto/platform6-ui-components"
    },
    "keywords": [
        "user-interface",
        "react",
        "typescript"
    ],
    "description": "Platform 6 UI components provided to develop the user interface of services.",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "bootstrap": "npm rebuild node-sass; lerna bootstrap --hoist",
        "build": "lerna run build",
        "test": "nyc ava --verbose",
        "update_p6_version": "node update_platform6_version.js",
        "prepare": "npm run bootstrap",
        "prepublish": "npm run bootstrap",
        "release": "lerna publish --yes --exact --registry=https://registry.npmjs.org/ --no-git-reset patch -- --production --access public",
        "release:version": "lerna publish --yes --registry=https://registry.npmjs.org/ --no-git-reset patch --exact -- --production --access public",
        "release:force": "lerna publish --yes --registry=https://registry.npmjs.org/ --no-git-reset patch --exact --force-publish=* -- --production --access public",
        "setup": "npm install",
        "styleguide": "npx styleguidist server --verbose",
        "styleguide:build": "styleguidist build"
    },
