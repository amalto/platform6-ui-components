    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/amalto/platform6-ui-components"
    },
    "keywords": [
        "user-interface",
        "react",
        "typescript"
    ],
    "description": "Platform 6 UI components provided to develop the user interface of services.",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "bootstrap": "lerna bootstrap --hoist",
        "build": "lerna run build",
        "test": "nyc ava --verbose",
        "update_p6_version": "node update_platform6_version.js",
        "prepare": "npm run bootstrap",
        "prepublish": "npm run bootstrap",
        "publish": "lerna publish --yes --exact --independent --since --cd-version patch -- --production --access public --registry=https://registry.npmjs.org/",
        "publish:version": "lerna publish --yes --exact --independent --since -- --production --access public --registry=https://registry.npmjs.org/",
        "publish:force": "lerna publish --yes --exact --independent --since --force-publish=* -- --production --access public --registry=https://registry.npmjs.org/",
        "setup": "npm install",
        "styleguide": "npx styleguidist server --verbose",
        "styleguide:build": "styleguidist build"
    },
